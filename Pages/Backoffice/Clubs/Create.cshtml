@page
@model Mercato.Pages.Backoffice.Clubs.Create
@{
    ViewData["Title"] = "Create";
}

<h1>Create a new club</h1>

<form method="post">
    <label for="Name">Name:</label>
    <input type="text" id="Name" name="Name" asp-for="Club.name" required>
    <br />
    <button type="submit">Create</button>
</form>